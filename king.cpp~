#include "king.h"

King::King() : Piece()
{
}

King::King(const string &_color, const Location &_location)
  : Piece(_color, _location)
{
}

King::~King()
{
}

bool King::validMove(const Location &_location)
{
  return false;
}

string King::display()
{
  return color + "K";
}
